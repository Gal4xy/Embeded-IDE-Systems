<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
     <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="runnotepad.NpLaunchConfigurationDelegate"
            category="org.eclipse.ui.externaltools"
            id="NpLaunchConfigurationType"
            modes="run, debug"
            name="Launch an exe">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="runnotepad.NpLaunchConfigurationTabGroup"
            id="NplaunchConfigurationTabGroup"
            type="NpLaunchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>

</plugin>
